<form class="form-wrapper" id="formParty" method="post">
  <div class="form-Intro">
    <h1>{{{ title }}}</h1>
  </div>
    {{{  helloUser   }}}

    <fieldset class="form-name">
      <legend>Sign In</legend>

      <div class="form-input-wrapper">
        <input id="userName" name="userName"  {{{ userName }}}  placeholder="Ivan Petrov" >
        <label for="userName"> User Name</label>
      </div>

        <div class="form-input-wrapper">
        <input type="password" id="password" name="password"  placeholder="password" >
        <label for="password"> Password </label>
      </div>

    </fieldset>

  <button id="submitBtn" class="form-submit-btn" type="submit"  value="Submit "> Sign In </button>

</form>
